{
  "name": "styleclueless",
  "version": "1.0.0",
  "description": "",
  "main": "index.2",
  "scripts": {
    "restart":"npm run dockerCleanImages && npm run installAll",
    "dockerCleanImages":"yes Y | docker image prune -a",
    "dockerCleanAll":"yes Y |  docker system prune --all",
    "installAll": "cd docker && docker-compose down && cd ../../client && npm i && npm run build && cd ../server/docker && docker-compose -f docker-compose.yml up -d --build && docker-compose up",
    "mongod": "mongod --dbpath ./db",
    "babel-node": "babel-node ./src/index.js",
    "nodemon": "nodemon --signal SIGINT --exec npx babel-node ./src/index.js",
    "yarn-parcel": "yarn parcel watch ./src/client/index.js",
    "build-client": "npx parcel build ./src/client/index.js",
    "build-server": "npx babel src --out-dir lib",
    "build-seed": "npx babel ./seed/download-csvs.js -o dist/seed.js",
    "build": "npm run build-client && npm run build-server && npm run build-seed",
    "dev": "npm run nodemon & npm run yarn-parcel",
    "start": "node dist/seed.js && node lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "babel-node ./seed/download-csvs.js",
    "cluster": "pm2 kill && pm2 start processes.json&& pm2 logs"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "seed/*",
      "public/*"
    ],
    "delay": "2500"
  },
  "author": "Naor Biton",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.7.1",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-link-http": "^1.5.16",
    "aws-sdk": "^2.656.0",
    "body-parser": "^1.19.0",
    "caller-id": "^0.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-prom-bundle": "^5.1.5",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "https": "^1.0.0",
    "koa": "^2.11.0",
    "koa-mount": "^4.0.0",
    "koa-react-router": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mongodb": "^3.3.3",
    "prom-client": "^11.5.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4"
  }
}
